      "lyrics": [
        { "id": "0-1", "text": "نازل و انا ماشى" },
        { "id": "0-2", "text": "\nع الشوك برجليا" },
        { "id": "0-3", "text": "\nو انت السبب يابا" },
        { "id": "0-4", "text": "\nو يالى خليت بيا " },
        { "id": "1-1", "text": "لا فرشتلى بستان" },
        { "id": "1-2", "text": "\nو لا حتى بر أمان" },
        { "id": "1-3", "text": "\nأعيش عليه إنسان" },
        { "id": "1-4", "text": "\nو الدنيا فى ايديا" },
        { "id": "1-5", "text": "\nفى ايديا" },
        { "id": "2-1", "text": "و انا لما جيت من الغيب" },
        { "id": "2-2", "text": "\nالعدل كان كيفى" },
        { "id": "2-3", "text": "\nو الحق كان مطمعى" },
        { "id": "2-4", "text": "\nو الحب كان سيفى" },
        { "id": "3-1", "text": "دقيت ع الأبواب" },
        { "id": "3-2", "text": "\nلقيتنى فى السرداب" },
        { "id": "3-3", "text": "\nغابة من الأنياب" },
        { "id": "3-4", "text": "\nمسنونة حواليا" },
        { "id": "4-1", "text": "مع الأسف يابا" },
        { "id": "4-2", "text": "\nالدنيا دى غابة" },
        { "id": "4-3", "text": "\nملناش مكان فيها" },
        { "id": "5-1", "text": "يا عينى يا وعدى" },
        { "id": "5-2", "text": "\nع المغرمين بعدى" },
        { "id": "5-3", "text": "\nو الطيبين فيها" }
      ],
      "title": "نازل و أنا ماشي"
    },
