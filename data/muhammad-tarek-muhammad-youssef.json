      "lyrics": [
        { "id": "0-1", "text": "مولاى صلّ و سلم دائما أبدا" },
        { "id": "0-2", "text": "\nعلى حبيبك خير الخلق كلهم" },
        { "id": "1-1", "text": "ﻳﺎ ﻧﺒﻲ ﺳﻼﻡ ﻋﻠﻴﻚ" },
        { "id": "1-2", "text": "\nﻳﺎﺭﺳﻮﻝ ﺳﻼﻡ ﻋﻠﻴﻚ" },
        { "id": "1-3", "text": "\nﻳﺎ ﺣﺒﻴﺐ ﺳﻼﻡ ﻋﻠﻴﻚ" },
        { "id": "1-4", "text": "\nﺻﻠﻮﺍﺕ ﺍﻟﻠﻪ ﻋﻠﻴﻚ" },
        { "id": "2-1", "text": "صلّ على النبي" },
        { "id": "2-2", "text": "\nوتبسم" },
        { "id": "2-3", "text": "\nده النبي تبسم" },
        { "id": "2-4", "text": "\nو تبسم" },
        { "id": "2-5", "text": "\nده النبي تبسم" },
        { "id": "2-6", "text": "\nاللهم صلّ عليه" },
        { "id": "3-1", "text": "قلبي المتيم كلما" },
        { "id": "3-2", "text": "\nصلى عليك وسلم" },
        { "id": "3-3", "text": "\nذاق السعادة مغرما" },
        { "id": "3-4", "text": "\nيا سيدي يا محمد" },
        { "id": "4-1", "text": "صلوا عليه شفيع الأمة" },
        { "id": "4-2", "text": "\nبذكر الرسول تشحذ الهمة" },
        { "id": "4-3", "text": "\nيا ربي بجاه النبي أزح الغمة" },
        { "id": "4-4", "text": "\nيا ربي بجاه النبي أزح الغمة" },
        { "id": "5-1", "text": "قمر قمر" },
        { "id": "5-2", "text": "\nقمر سيدنا النبي" },
        { "id": "5-3", "text": "\nوجميل وجميل" },
        { "id": "5-4", "text": "\nوجميل سيدنا النبي وجميل" },
        { "id": "6-1", "text": "محمد نبينا" },
        { "id": "6-2", "text": "\nبنوره هادينا" },
        { "id": "6-3", "text": "\nمن مكة حبيبى نوره" },
        { "id": "6-4", "text": "\nساطع ع المدينة" },
        { "id": "6-5", "text": "\nمن صلى صلاته" },
